{
  "name": "spadespromise",
  "version": "1.0.0",
  "description": "A promisify library that based on ES6 native implementation of Promise",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require blanket --reporter doc > coverage.html"
  },
  "config": {
    "blanket": {
      "pattern": "promisify.js",
      "data-cover-never": [
        "node_modules",
        "test"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spade69/spadesPromise.git"
  },
  "keywords": [
    "JavaScript",
    "ES6",
    "Promisify"
  ],
  "author": "Jason",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spade69/spadesPromise/issues"
  },
  "homepage": "https://github.com/spade69/spadesPromise#readme",
  "devDependencies": {
    "blanket": "^1.2.3",
    "chai": "^3.5.0",
    "mocha": "^3.1.2"
  }
}
